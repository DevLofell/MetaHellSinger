"VertexLitGeneric"
{
	"$basetexture" "models\Metal Hellsinger\Characters\The Unknown/Metal_D"
	"$bumpmap" "models\Metal Hellsinger\Characters\The Unknown/Metal_N"
	"$phong" "1"
	"$envmap" "models\Metal Hellsinger\Characters\The Unknown/Cave"
               "$envmapfresnel" "1"
               "$envmaptint" "[ .1 .1 .1 ]"
	"$nocull" "1"
	$phong 1
	$phongboost 10
	$phongexponent 40
	$phongfresnelranges "[.5 .8 1]"


	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "50"	
	"$rimlightboost" "0"

	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		  "Sine"
	    {
	        "resultVar"		"$Detailblendfactor"
	        "SinePeriod"	"2"
	        "SineMin"		"0"
	        "SineMax"		"2.5"
	   }
}